{
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": {
         "32": "img/icon-32x32.png"
      },
      "default_title": "Extension"
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "",
   "homepage_url": "https://github.com/Autoraidapi/extension/",
   "icons": {
      "128": "img/icon-128x128.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18.0",
   "name": "Extension",
   "offline_enabled": true,
   "options_page": "options.html",
   "permissions": [
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "https://*/"
   ],
   "version": "1.0.0",
   "web_accessible_resources": ["options.js"]
}
